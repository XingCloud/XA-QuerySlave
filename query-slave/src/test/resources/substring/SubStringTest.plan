{
  head:{
    type:"apache_drill_logical_plan",
    version:"1",
    generator:{
      type:"manual",
      info:"na"
    }
  },
  storage:[
    {
      type:"console",
      name:"console"
    },
    {
      type:"fs",
      name:"fs1",
      root:"file:///"
    },
    {
      type:"classpath",
      name:"cp"
    },
     {
       type: "queue",
       name: "queue"
     }
  ],
  query:[
    {
      op:"sequence",
      do:[
	    {
	      op: "scan",
	      memo: "initial_scan",
	      ref: "donuts",
	      storageengine: "cp",
	      selection: {
	        path: "/substring/donuts.json",
	        type: "JSON"
	      }
	    },
        {
	      op: "transform",
	      transforms: [
	        { ref: "shortid", expr: "substring(donuts.id, 2, 4)"}
	      ]
	    },
      {
        op: "segment",
        ref: "ppusegment",
        exprs: ["donuts.ppu"]
      },
      {
        op: "collapsingaggregate",
        within: "ppusegment",
        carryovers: ["shortid"],
        aggregations: [
          { ref: "count.distinct.substring.donuts.id",  expr: "countDistinct(substring(donuts.id, 0, 4))" }
        ]
      },
	    {
	      op: "order",
	      orderings: [
	        {order: "desc", expr: "donuts.ppu" }
	      ]
	    },
	    {
	      op: "store",
	      memo: "output sink",
	      storageengine: "queue",
	      target: {number:0}
	    }
      ]
    }
  ]
}